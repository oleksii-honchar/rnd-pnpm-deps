    "tsc-alias": "^1.8.8",
    "tsc-watch": "^6.0.4",
    "tslib": "^2.6.2",
    "typedoc": "0.25.12",
    "typedoc-plugin-markdown": "4.0.0-next.53",
    "typescript": "5.4.2"
  }
